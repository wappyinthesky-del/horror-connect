{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "horror-connect",
  "compatibility_date": "2025-08-25",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "NODE_ENV": "production"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "horror-connect-production",
      "database_id": "WILL_BE_SET_AFTER_CREATION"
    }
  ]
}